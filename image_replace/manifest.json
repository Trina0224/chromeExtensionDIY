{
    "manifest_version": 2,
    "name": "Image replacer", 
    "version": "0.0.1",
    "description": "Changes images on a website",
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    }, 
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": ["*.jpg", "*.JPG", "*" ]
}