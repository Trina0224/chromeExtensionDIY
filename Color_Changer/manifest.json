{
    "manifest_version": 2,
    "name": "Color changer",
    "version": "0.0.1",
  
    "description": "Changes background color",
    "icons":{ 
        "16": "images/iconfinder_In-yang_132767.png",
        "32": "images/iconfinder_Diagram_131845.png",
        "48": "images/iconfinder_Sun_132166.png",
       "128": "images/Default-Icon-icon.png" 
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["storage","activeTab", "*://*/*"],
    "options_page": "options.html",
    "browser_action": {
        "default_icon":{
            "16": "images/icon16.png",
            "32": "images/icon32.png"        
        },
        "default_title": "Hi Trina",
        "default_popup": "popup.html"  
    }

  }